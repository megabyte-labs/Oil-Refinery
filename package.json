  "lint-staged": {
    "**/*": [
      "task lint:any --"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.sh": [
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "main": "dist/main.js",
  "name": "@mblabs/app-oil-refinery",
  "ntl": {
    "descriptions": {
      "help": "Display a list of tasks that can be run via the Taskfile.yml",
      "prepare": "Hook, triggered by running npm install, that runs logic such as installing pre-commit hooks",
      "repair": "Fixes basic issues in projects that are having trouble running basic commands like 'task start'",
      "start": "Entrypoint for new developers that installs requirements and then runs 'task start'"
    }
  },
  "optionalDependencies": {},
  "prettier": "@washingtondc/prettier",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "extends": "@washingtondc/release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorManhattan/Oil-Refinery.git"
  },
  "scripts": {
    "build": "ng build",
    "help": "task --list",
    "ng": "ng",
    "prepare": "task common:husky",
    "repair": "bash <(curl -sS https://install.doctor/repair)",
    "start": "ng serve",
    "test": "ng test",
    "test:cov": "ng test --coverage",
    "watch": "ng build --watch --configuration development"
  },
  "type": "module",
  "typings": "dist/main.d.ts",
  "version": "0.0.1"
